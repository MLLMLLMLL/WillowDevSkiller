<Window x:Class="WillowDevSkiller.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WillowDevSkiller"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Label Content="Dev Skiller Stuff" HorizontalAlignment="Left" Margin="15,15,0,0"/>
            <Button x:Name="btnDevSkiller" Content="Do Stuff" Width="50" Height="30" HorizontalAlignment="Left" Margin="15,0,15,0" Click="btnDevSkiller_Click"/>
        </StackPanel>

    </Grid>
</Window>
